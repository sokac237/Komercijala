                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select robnagrupa.RG_SIFRA, robnagrupa.RG_NAZIV, artikl.AR_SIFRA, artikl.AR_NAZIV, narudzbastavka.*, artikl.ar_nenarucuj
from  narudzbastavka
