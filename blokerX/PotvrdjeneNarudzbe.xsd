                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select narudzba.*,
b1.po_naziv as Skladiste,
b2.po_naziv as Poslovnica,
b1.po_sifra as SifraSkladista,
b2.po_sifra as SifraPoslovnice,
concat(c1.op_ime ,' ',c1.op_prezime)   as Komercijalist,
concat(c2.op_ime ,' ',c2.op_prezime) as Operater
